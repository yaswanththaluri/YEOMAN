<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="androidapp.yashthaluri.com.yeoman">

    <application
        android:allowBackup="true"
        android:icon="@drawable/leaf"
        android:label="@string/app_name"
        android:roundIcon="@drawable/leaf"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".Activities.MarkAbsentActivity"></activity>
        <activity android:name=".Activities.DetailWorkersShowActivity" />
        <activity android:name=".Activities.MainActivity" />
        <activity android:name=".Activities.Authentication" />
        <activity android:name=".Activities.WelcomeActivity" />
        <activity android:name=".Activities.BookActivity" />
        <activity android:name=".Activities.ProfileActivity" />
        <activity android:name=".Activities.EditProfileDetailActivity" />
        <activity
            android:name=".Activities.SplashScreenActivity"
            android:hardwareAccelerated="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <!--  -->
            </intent-filter>
        </activity>
    </application>

</manifest>